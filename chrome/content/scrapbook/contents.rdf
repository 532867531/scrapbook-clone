<?xml version="1.0" ?>

<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

	<!--declare package-->
	<RDF:Seq about="urn:mozilla:package:root">
		<RDF:li resource="urn:mozilla:package:scrapbook" />
	</RDF:Seq>

	<!--description about package-->
	<RDF:Description about="urn:mozilla:package:scrapbook"
	                 chrome:displayName="ScrapBook"
	                 chrome:author="Gomita"
	                 chrome:authorURL="http://amb.vis.ne.jp/mozilla/scrapbook/"
	                 chrome:settingURL="chrome://scrapbook/content/setting.xul"
	                 chrome:name="scrapbook"
	                 chrome:description="This extension helps you to save Web pages and easily manage collections."
	                 chrome:extension="true">
	</RDF:Description>

	<!--overlay to browser-->
	<RDF:Seq about="urn:mozilla:overlays">
		<RDF:li resource="chrome://browser/content/browser.xul" />
	</RDF:Seq>

	<RDF:Seq about="chrome://browser/content/browser.xul">
		<RDF:li>chrome://scrapbook/content/overlay.xul</RDF:li>
	</RDF:Seq>

</RDF:RDF>
